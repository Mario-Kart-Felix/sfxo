{
  "name": "sfxo",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "preact-scripts": "0.3.0-alpha",
    "react-test-renderer": "^15.3.0"
  },
  "dependencies": {
    "preact": "^5.6.0"
  },
  "scripts": {
    "start": "preact-scripts start",
    "build": "preact-scripts build",
    "eject": "preact-scripts eject",
    "test": "preact-scripts test",
    "create-sprite": "sprity create ./src/ ./images/*png -s App.css --no-sort",
    "create-sprite-mobile": "sprity create ./src/ ./images-mobile/*png -n sprite-mobile -s App.mobile.css --no-sort",
    "deploy": "npm run build && surge ./build"
  },
  "eslintConfig": {
    "extends": "./node_modules/preact-scripts/config/eslint.js"
  }
}